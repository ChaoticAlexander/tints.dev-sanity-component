{
  "name": "tints.dev-sanity-component",
  "type": "module",
  "description": "A 10-swatch palette generator primarily for Tailwind CSS",
  "license": "MIT",
  "version": "2.0.0",
  "author": "Simeon Griggs",
  "files": [
    "dist"
  ],
  "main": "./dist/tints-dev-components.umd.js",
  "module": "./dist/tints-dev-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/tints-dev-components.es.js",
      "require": "./dist/tints-dev-components.umd.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:css && npm run build:js",
    "build:js": "vite build",
    "lint": "eslint --fix",
    "build:css": "tailwindcss -o src/styles.css",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install",
    "prepare": "npm run build"
